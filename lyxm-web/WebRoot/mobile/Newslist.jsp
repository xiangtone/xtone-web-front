    <script>
       $(function(){

    var isloading = false;
    $(".more").click(function () {
        if (isloading)
            return;
        isloading = true;

        var me = $(this),
            id = me.attr("data-id"),
            current_page = me.data("page");
        current_page = current_page ? current_page : 1;

        $.ajax({
            type:"post",
            url:"/gameInfo/getsynth",
            data: {"page":current_page+1, "id": id}, 
           success: function (res) {

                res=eval("("+res+")");
                if (res.article_list.length == 0) {

                    me.hide();
                }
                for (var i = 0; i < res.article_list.length; i++) {
                    me.closest("li.active").find("ul").append(buildHtml(res.article_list[i]));
                }
                me.data("page", current_page + 1);
                isloading = false;
            },
            error: function(e){
                console.log(e);
            }
        });
    });
});

  

function buildHtml(res) {
    var html = "";
    html = '<li><a href="/article/' + res.id + '">'+res.title+'</a><span></span></li>';
    return html;
}
    </script>





