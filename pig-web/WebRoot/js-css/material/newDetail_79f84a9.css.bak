@charset "gb2312";

html,body,fieldset,img,iframe,abbr {
	border:0
}
li {
	list-style:none
}
textarea {
	overflow:auto;
	resize:none
}
a,button {
	cursor:pointer
}
h1,h2,h3,h4,h5,h6,em,strong,b {
	font-weight:700;
	font-style: normal;
}
a,a:hover {
	text-decoration:none
}

html,body {
	width:100%;
	min-height:930px;
	height:auto
}
.hide {
	display:none
}

body .wrap {
	background: transparent url(./img/bg_1.png) no-repeat scroll center 0px;
	position:absolute;
	width:100%;
	min-width:1280px;
	min-height:1250px;
	height:auto;
	background:url(./img/bg_1.png) center 0 no-repeat;
}
.wrap .right-wrap {
	width:941px;
	height:100%;
	margin-left: 25%;
}
body .wrap .right-wrap .content-wrap {
	width:825px;
	height:auto;
	min-height:955px;
	margin:93px 0 0 116px;
	background-color:#fff
}
body .wrap .right-wrap .content-wrap .content_title {
	position:relative;
	width:100%;
	height:86px;
	background:url() center 0 no-repeat
}
body .wrap .right-wrap .content-wrap .content_title .title {
	position:absolute;
	top:29px;
	left:23px;
	width:200px;
	height:30px
}
body .wrap .right-wrap .content-wrap .content_title span {
	position:absolute;
	top:46px;
	right:32px;
	font-size:14px;
	color:#6e6e6e
}
body .wrap .right-wrap .content-wrap .content_title span a {
	color:#6e6e6e
}
body .wrap .right-wrap .content-wrap .content_title span a:hover {
	color:#A99359
}
body .wrap .right-wrap .content-wrap #NIE-art {
	width:757px;
	padding:58px 34px 0;
	text-align:center
}
body .wrap .right-wrap .content-wrap #NIE-art .artTitle {
	font-size:24px;
	color:#a27c64
}
body .wrap .right-wrap .content-wrap #NIE-art .artDate {
	margin-top:10px;
	font-family:"微软雅黑";
	font-size:14px;
	color:#5c5552;
	display:inline-block
}
body .wrap .right-wrap .content-wrap #NIE-art .artText {
	text-align:center;
	margin-top:15px
}
body .wrap .right-wrap .content-wrap #NIE-art .artText h2 {
	font-size:20px;
	color:#6e6e6e;
	margin-top:15px;
	text-align:left
}
body .wrap .right-wrap .content-wrap #NIE-art .artText p {
	text-align:left;
	font-size:14px;
	color:#3C2D2D;
	line-height:27px
}
body .wrap .right-wrap .content-wrap #NIE-art .artText p a {
	color:#a99359
}
body .wrap .right-wrap .content-wrap #NIE-art .artText p a:hover {
	text-decoration:underline
}
body .wrap .right-wrap .content-wrap #NIE-art .artText h3 {
	font-size:16px;
	color:#838383;
	margin-top:15px;
	text-align:left
}
body .wrap .right-wrap .content-wrap #NIE-art .table-pic td {
	border:1px #a99359 solid
}
body .wrap .right-wrap .content-wrap #NIE-art .table-txt thead {
	background-color:#aaaaaa;
	color:#fff
}
body .wrap .right-wrap .content-wrap #NIE-art .table-txt td {
	border:1px #a99359 solid
}
body .wrap .right-wrap .content-wrap .content_list_wrap {
	width:761px;
	margin-left:33px
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul {
	width:100%;
	margin-top:11px
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul .content_list {
	position:relative;
	width:100%;
	height:59px;
	font-size:14px;
	color:#aba49a;
	line-height:70px;
	border-bottom:1px solid #ebedee
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul .content_list a {
	color:#aba49a
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul .content_list a:hover {
	color:#A99359
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul .content_list span {
	position:absolute;
	right:0
}
body .wrap .right-wrap .content-wrap .content_list_wrap ul .lastList {
	border:0
}
body .wrap .right-wrap .content-wrap .pageNum {
	width:auto;
	height:27px;
	margin-right:33px;
	text-align:right;
	margin-top:13px
}
body .wrap .right-wrap .content-wrap .pageNum p {
	width:100%;
	height:100%;
	color:#5c5653;
	padding-top:2px
}
body .wrap .right-wrap .content-wrap .pageNum p a {
	font-size:14px;
	padding:4px 9px;
	margin-right:5px;
	color:#5c5653;
	border:1px solid #ebedee
}
body .wrap .right-wrap #NIE-share {
	margin:53px 0 0 116px
}
body .wrap .right-wrap #NIE-share .NIE-share-txt {
	font-size:14px;
	color:#737C86;
	margin-right:10px
}
body .wrap .right-wrap #NIE-share .NIE-share-iconBtn {
	top:10px
}
body .wrap .right-wrap #NIE-share .NIE-share-iconBtn a {
	color:#767676;
	margin-right:10px
}
body .wrap .right-wrap #NIE-share .morebtn {
	display:none!important
}
body .wrap .right-wrap .copyRight-bg {
	color:#737C86
}
body .wrap .right-wrap .copyRight-bg a {
	color:#737C86
}
body {
	background-color:#aaa;
}
body .wrap .right-wrap .content-wrap .content_title .title {
	background:url(./img/material.png) center 0 no-repeat
}
